    <PreferenceCategory>
        <EditTextPreference
            android:defaultValue="https://app.mdt.mt.gov/arcgis/rest/services/hsip/FeatureServer/1"
            android:key="featurelayerurl"
            android:summary="Rest endpoint of Feature Layer."
            android:title="Feature Layer URL" />
        <EditTextPreference
            android:defaultValue="https://app.mdt.mt.gov/arcgis/rest/services/hsip/FeatureServer/0"
            android:key="medialayerurl"
            android:summary="Rest endpoint of Media Layer."
            android:title="Media Layer URL" />
        <EditTextPreference
            android:defaultValue="mdt/OfflineData"
            android:dialogTitle="storage area"
            android:key="storagepath"
            android:summary="Path to store offline data."
            android:title="Storage Path" />
        <EditTextPreference
            android:defaultValue="file:///mnt/sdcard/mdt/Starter_Map/Layers"
            android:key="offlinemapcache"
            android:summary="Location of offline maps (.tpk or tiles).  i.e. file:///mnt/sdcard/...."
            android:title="Offline Map Cache" />
        <EditTextPreference
            android:key="username"
            android:summary="Please maintain your username"
            android:title="Username" />
        <EditTextPreference
            android:inputType="textPassword"
            android:key="password"
            android:summary="Please maintain your password"
            android:title="Password" />
    </PreferenceCategory>
    
</PreferenceScreen>